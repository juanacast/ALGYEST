#include <iostream>

int main() {
  int num, *dir_num; 
  //dir_num es la direccion de memoria de num
  
  num = 20;
  dir_num = &num;

  std:: cout<< "Numero: " << *dir_num<<std::endl;
  //Esto me devuelve el numero osea 20, ahora como hago para que devuelva 
  // la direccion de memoria donde se almacena ese numero

  std:: cout<<"Direccion de memoria donde se almacena num: " <<dir_num << std::endl;
  //Entonces & indica la direccion de memoria donde esta guardada esa variable. 

  return 0;
}
